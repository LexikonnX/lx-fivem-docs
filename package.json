{
  "name": "lx-fivem-docs",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "lx-fivem-docs": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test-scan": "node dist/index.js scan ./resources --out ./docs --format md,json"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "8.8.1",
    "acorn": "8.12.1",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "glob": "11.0.0",
    "gray-matter": "4.0.3",
    "luaparse": "0.3.1",
    "pretty-bytes": "6.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "tsup": "8.3.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3"
  }
}
